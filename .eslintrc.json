{
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es2021": true
    },
    "rules": {
      "indent": "off",
      "linebreak-style": "off",
      "quotes": "off",
      "semi": "off",
      "comma-dangle": "off",
      "max-len": "off",
      "no-trailing-spaces": "off",
      "eol-last": "off",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "valid-typeof": "error",
      "no-const-assign": "error",
      "no-dupe-args": "error",
      "no-dupe-keys": "error"
    }
  }